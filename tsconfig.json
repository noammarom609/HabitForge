{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "App.tsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "convex/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "app-example",
    "clerk-expo"
  ]
}
